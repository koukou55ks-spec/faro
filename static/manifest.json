{
  "name": "Unloq - 知識を解き放つAIパートナー",
  "short_name": "Unloq",
  "description": "複雑な情報を分かりやすく、あなたの学習と意思決定をサポートするAI駆動プラットフォーム",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#18181b",
  "orientation": "portrait-primary",
  "categories": ["education", "productivity", "utilities"],
  "lang": "ja",
  "dir": "ltr",
  "icons": [
    {
      "src": "/static/icons/icon.svg",
      "sizes": "any",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "/static/favicon.svg",
      "sizes": "any",
      "type": "image/svg+xml",
      "purpose": "maskable any"
    }
  ],
  "shortcuts": [
    {
      "name": "新しい質問",
      "short_name": "質問",
      "description": "AIに新しい質問をする",
      "url": "/?action=new_question"
    },
    {
      "name": "学習履歴",
      "short_name": "履歴",
      "description": "過去の学習履歴を確認",
      "url": "/?action=history"
    }
  ],
  "screenshots": [],
  "prefer_related_applications": false,
  "related_applications": [],
  "scope": "/",
  "id": "unloq-ai-partner",
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "handle_links": "preferred",
  "edge_side_panel": {
    "preferred_width": 400
  },
  "protocol_handlers": [
    {
      "protocol": "web+unloq",
      "url": "/?query=%s"
    }
  ]
}
